source 'https://rubygems.org'

gem 'rails', '3.2.8'
gem "railties", "~> 3.2.8"
gem 'bootstrap-sass', '~> 2.1.0.0'
gem 'jquery-rails', '2.0.2'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails'
  gem 'coffee-rails'
  gem 'uglifier'
end

# Gems used only in the development environment and not required in the production environment
group :development do
  gem 'sqlite3', '1.3.5'
end





